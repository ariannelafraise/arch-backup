{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Shell }} - {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": " {{ .Icon }} ",
          "foreground": "#dc9ffc",
          "type": "os",
          "style": "plain"
        },
        {
          "template": " {{ .Path }} ",
          "foreground": "#FF88C2",
          "type": "path",
          "style": "plain"
        },
        {
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#FF88C2",
          "type": "git",
          "style": "plain"
        },
        {
          "template": " {{ .FormattedMs }} ",
          "foreground": "#FF88C2",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "template": " \uf08a{{ if gt .Code 0 }} {{ .Code }}{{ else }} ❯{{ end }} ",
          "foreground": "#FF88C2",
          "type": "status",
          "style": "plain"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "template": " \ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "foreground": "#FF88C2",
          "type": "python",
          "style": "plain"
        },
        {
          "template": " \ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#FF88C2",
          "type": "go",
          "style": "plain"
        },
        {
          "template": " \ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#FF88C2",
          "type": "node",
          "style": "plain"
        },
        {
          "template": " \ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#FF88C2",
          "type": "ruby",
          "style": "plain"
        },
        {
          "template": " \ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#FF88C2",
          "type": "java",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u25a0",
          "foreground": "#55CDFC",
          "type": "text",
          "style": "plain"
        },
        {
          "template": "\u25a0",
          "foreground": "#F7A8B8",
          "type": "text",
          "style": "plain"
        },
        {
          "template": "\u25a0",
          "foreground": "#FFFFFF",
          "type": "text",
          "style": "plain"
        },
        {
          "template": "\u25a0",
          "foreground": "#F7A8B8",
          "type": "text",
          "style": "plain"
        },
        {
          "template": "\u25a0",
          "foreground": "#55CDFC",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": " ❯ ",
          "foreground": "#FF88C2",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 4,
  "final_space": true
}
